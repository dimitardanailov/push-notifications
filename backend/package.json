  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dimitardanailov/push-notifications/issues"
  },
  "homepage": "https://github.com/dimitardanailov/push-notifications#readme",
  "dependencies": {
    "esm": "^3.2.25",
    "express": "^4.18.2",
    "web-push": "^3.3.5"
  },
  "devDependencies": {
    "dotenv": "^8.1.0",
    "nodemon": "^1.19.1"
  }
}
